!!! 5html  head    title Login Page    link(rel='stylesheet', href='/stylesheets/style.css')    script(type='text/javascript', src='javascripts/lib/jquery.js')    script(type='text/javascript', src='javascripts/lib/moment.js')    script(src='http://localhost:3000/socket.io/socket.io.js')    script(type='text/javascript', src='javascripts/util/utility.js')    script(type='text/javascript', src='javascripts/page/list.js')    script(src='/javascripts/page/competitionSocket.js')  body(oncontextmenu='return false;')    section.loginform.np.lt      div#divList        ul          li.divider Competitions(#{competitions.length})          div#com-container            if(competitions.length<1)              span.war No Competition avilable for you right now.            else              each com, i in competitions                li(class='competition', id='#{com.id}', onclick='openComp(this.id);').                   span(class='logo') #{com.competition_name}                  span(class='status') #{com.status}                  span(class='statusText') #{com.statusText}          li.divider Tests(#{tests.length})          div#test-container            if(tests.length<1)              span.war No Test avilable for you right now.            else              each test, i in tests                li(class='test', id='#{test.id}', onclick='openTest(this.id);').                    span(class='logo') #{test.test_name}                  span(class='status') #{test.status}                  span(class='statusText') #{test.statusText}          li.divider Personal Games(#{personal_games.length})            input#btnCrGame(type='button', value='Create Game', class='button right-btn')          div#game-container            ul              if(personal_games.length<1)                span.war You have not created any game.              else                each game, i in personal_games                  li(class='game', id='#{game.id}', onclick='OpenGame(this.id);')                    span(class='logo') #{game.name}                    span(class='status') #{game.time} min                    span(class='statusText') Highest Score #{game.highestScore}      div(id='divCreateGame', class='create-game')		        input#txtGameName(type='text', name='txtGameName', placeholder='Game Name')        input#txtGameTime.middle-text(type='text', name='txtGameTime', placeholder='Time')        span.bold-text min        span(class='radio', id='limitTargetTime')        span.allow           b Allow equal time target        input#btnCreateLevel.button.right-btn(type='button', value='Add Levels')        .level-cont          ul        input#btnCancelGame.button.right-btn(type='button', value='Cancel')        input#btnSaveGame.button.right-btn(type='button', value='Save Game')      div(id='divCreateLevel', class='create-game')	        input#txtLevelName(type='text', placeholder='Level Name')        input#txtLevelTargets(type='text', placeholder='Total Targets')        span.heading Operators        .fun-cont          ul            li              span.radio              .level-name                 span !                span (Factorial)            li              span.radio              .level-name                 span ^                span (Power)            li              span.radio              .level-name                 span +                span (Addition)            li              span.radio              .level-name                 span -                span (Substraction)            li              span.radio              .level-name                 span *                span (Multiply)            li              span.radio              .level-name                 span /                span (Divide)        span.heading Operands        .card-cont          ul            li              span.radio              .level-name                 span 1                span (One)            li              span.radio              .level-name                 span 2                span (Two)            li              span.radio              .level-name                 span 3                span (Three)            li              span.radio              .level-name                 span 4                span (Four)            li              span.radio              .level-name                 span 5                span (Five)            li              span.radio              .level-name                 span 6                span (Six)            li              span.radio              .level-name                 span 7                span (Seven)            li              span.radio              .level-name                 span 8                span (Eight)            li              span.radio              .level-name                 span 9                span (Nine)            li              span.radio              .level-name                 span 10                span (Ten)            li              span.radio              .level-name                 span 11                span (Eleven)            li              span.radio              .level-name                 span 12                span (Twelve)	        input#btnCancelLevel.button.right-btn(type='button', value='Cancel')        input#btnAddLevel.button.right-btn(type='button', value='Add Level')      div#pop      div#overlay    input(type='hidden', value='#{player.id}', id='hPlayerID')